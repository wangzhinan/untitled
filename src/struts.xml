<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    
    <package name="default" extends="struts-default" namespace="/">
        <action name="user_save" method="save" class="userAction"/>
        <action name="user_findAll" method="findAll" class="userAction">
            <result name="success">index.jsp</result>
        </action>
        <action name="test" method="execute" class="com.sunland.action.OutputAction"/>
        <action name="fileUpload" method="fileUpload" class="com.sunland.action.OutputAction"/>
    </package>
</struts>